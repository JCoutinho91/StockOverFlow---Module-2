<h1>Latest new's from the market</h1>
<button class="btn btn-outline-secondary"> <a href="javascript:history.back()"> Back </a></button>
<div class="row row-cols-1 row-cols-md-3 g-4">
  {{#each newsData}}
    <div class="col d-flex justify-content-center">
      <div class="card" style="width: 18rem;">
        <img src="{{this.urlToImage}}" class="card-img-top" alt="newsimg" />
        <div class="card-body">
          <h5 class="card-title">{{this.description}}</h5>
          <p class="card-text">{{this.description}}</p>
          <a href="{{this.url}}" class="btn btn-primary">Check More Here</a>
        </div>
      </div>
    </div>
  {{/each}}
</div>