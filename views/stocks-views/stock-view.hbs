<h1>Company List</h1>

<hr />
{{#each stockList}}
  <p>Ticker Symbol:{{this.data.ticker}}</p>
  <p>Company Name:{{this.data.companyInformation.shortName}}</p>
  <p>Current Price:{{this.data.priceData.currentPrice}}</p>
  <p>MarketCap: {{this.data.companyInformation.marketCap}}</p>
  <img src="{{this.data.companyInformation.logoURL}}" alt="" srcset="" />
  <hr />
{{/each}}

<a href="/stock-details">VIEW
  {{values.companyInformation.shortName}}
  Details</a>

